{
  "name": "Chrome extension boilerplate",
  "background": {
    "scripts": ["background.bundle.js"]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://linkedin.com/*",
        "*://*.linkedin.com/*"
      ],
      "js": ["content.bundle.js"],
      "run_at": "document_end"
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "icons": {
    "32": "icon-32.png",
    "128": "icon-128.png"
  },
  "manifest_version": 2,
  "permissions": [
    "storage"
  ],
  "version": "2.0.1"
}
